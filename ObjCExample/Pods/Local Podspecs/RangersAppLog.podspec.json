  "subspecs": [
    {
      "name": "Core",
      "source_files": "RangersAppLog/Core/*.{h,m,c}",
      "vendored_libraries": "RangersAppLog/Core/*.a",
      "frameworks": [
        "UIKit",
        "Foundation",
        "Security",
        "AdSupport",
        "CoreTelephony",
        "CoreFoundation",
        "SystemConfiguration",
        "CoreGraphics"
      ],
      "libraries": [
        "z",
        "sqlite3"
      ],
      "public_header_files": "RangersAppLog/Core/*.h"
    },
    {
      "name": "Host",
      "dependencies": {
        "RangersAppLog/Core": [

        ]
      },
      "subspecs": [
        {
          "name": "CN",
          "source_files": "RangersAppLog/Host_CN/*.{h,m,c}",
          "public_header_files": "RangersAppLog/Host_CN/*.h",
          "vendored_libraries": "RangersAppLog/Host_CN/*.a"
        },
        {
          "name": "SG",
          "source_files": "RangersAppLog/Host_SG/*.{h,m,c}",
          "public_header_files": "RangersAppLog/Host_SG/*.h",
          "vendored_libraries": "RangersAppLog/Host_SG/*.a"
        },
        {
          "name": "VA",
          "source_files": "RangersAppLog/Host_VA/*.{h,m,c}",
          "public_header_files": "RangersAppLog/Host_VA/*.h",
          "vendored_libraries": "RangersAppLog/Host_VA/*.a"
        }
      ]
    },
    {
      "name": "Log",
      "vendored_libraries": "RangersAppLog/Log/*.a",
      "dependencies": {
        "RangersAppLog/Core": [

        ]
      }
    },
    {
      "name": "Unique",
      "frameworks": "AdSupport",
      "dependencies": {
        "RangersAppLog/Core": [

        ]
      },
      "source_files": "RangersAppLog/Unique/*.{h,m,c}",
      "public_header_files": "RangersAppLog/Unique/*.h",
      "vendored_libraries": "RangersAppLog/Unique/*.a"
    },
    {
      "name": "UITracker",
      "vendored_libraries": "RangersAppLog/UITracker/*.a",
      "source_files": "RangersAppLog/UITracker/*.{h,m,c}",
      "frameworks": [
        "Foundation",
        "UIKit",
        "WebKit"
      ],
      "public_header_files": "RangersAppLog/UITracker/*.h",
      "dependencies": {
        "RangersAppLog/Core": [

        ]
      }
    },
    {
      "name": "Picker",
      "vendored_libraries": "RangersAppLog/Picker/*.a",
      "frameworks": [
        "Foundation",
        "UIKit",
        "WebKit",
        "CoreText"
      ],
      "dependencies": {
        "RangersAppLog/Log": [

        ],
        "RangersAppLog/UITracker": [

        ]
      },
      "source_files": "RangersAppLog/Picker/*.{h,m,c}",
      "public_header_files": "RangersAppLog/Picker/*.h",
      "resource_bundles": {
        "RangersAppLog": [
          "RangersAppLog/Assets/*.xcassets"
        ]
      }
    }
  ]
}
